package com.unitau.tgvinicius.dto;

public class UserDTO {
    private String id;
    private String login;
    private String name;

    public UserDTO() {}

    public UserDTO(String id, String login, String name) {
        this.id = id;
        this.login = login;
        this.name = name;
    }

    // Getters e Setters
}
